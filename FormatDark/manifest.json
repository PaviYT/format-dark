{
	"version": "1.0.13",
	"name": "Format: Dark",
	"short_name": "Format: Dark",
	"description": "Save the eys, darkify",

	"icons": {
		"128": "/images/icons/icon_128x128.png",
		"16": "/images/icons/icon_16x16.png",
		"48": "/images/icons/icon_64x64.png"
	},

	"content_scripts": [
		{
			"js": [
				"/lib/ext.js",
				"/lib/jquery-3.2.1.min.js",
				"/lib/highlight.pack.js",
				"/lib/global.js",

				"/content/themes.js",
				"/content/preloadDark.js"
			],
			"css": [
				"/css/page-dark-common.css",
				"/css/code/vs-dark-common.css",
				"/css/code/vs-dark-json.css",
				"/css/code/vs-dark-javascript.css",
				"/css/code/vs-dark-css.css"
			],
			"matches": [ "<all_urls>" ],
			"run_at": "document_start"
		},
		{
			"js": [
				"/content/addDarkClass.js"
			],
			"matches": [
				"*://gabrielecirulli.github.io/2048/"
			],
			"run_at": "document_start"
		},
		{
			"css": [
				"/css/games/2048-dark.css"
			],
			"matches": [ "*://gabrielecirulli.github.io/2048/*" ],
			"run_at": "document_start"
		}
	],

	"manifest_version": 2,
	"incognito": "split"
}